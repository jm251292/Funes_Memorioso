DELIMITER $$

CREATE PROCEDURE `login` (in nic varchar(100), in pass varchar(100), in roll varchar(100))
BEGIN
	select count(*) from usuario where nick = nic and contrase√±a = md5(pass) and rol = roll and bloqueado = 0;
END